   content: "";

   position: absolute;

   z-index: 1;

   /* same z-index as fc-bg, behind text */

   /* overcome the borders */

   top: -1px;

   right: -1px;

   bottom: -1px;

   left: -1px;

   /* darkening effect */

   background: #000;

   opacity: .25;

}



